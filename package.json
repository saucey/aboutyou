  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "test:local": "yarn test -- --watch --browsers Chrome",
    "test:ci": "yarn test -- --watch=false --progress=false --reporters=\"junit\" --code-coverage",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "start": "npm-run-all -p server:serve app:serve",
    "start:tdd": "npm-run-all -p test:local start",
    "app:serve": "ng serve",
    "version": "node scripts/version.js",
    "server:serve": "rimraf ./dist/server && npm run webpack:server:dev",
    "server:build": "rimraf ./dist/server && npm run webpack:server",
    "server:build:integration": "rimraf ./dist/server && npm run webpack:server -- --env.environment=integration",
    "server:build:staging": "rimraf ./dist/server && npm run webpack:server -- --env.environment=staging",
    "server:build:production": "rimraf ./dist/server && npm run webpack:server -- --env.environment=production",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "webpack:server:dev": "webpack --config webpack.server.config.js --mode development --progress --colors",
    "build:integration": "ng build --configuration=integration && npm run server:build:integration",
    "build:staging": "ng build --configuration=staging && npm run server:build:staging",
    "build:production": "ng build --prod && npm run server:build:production",
    "serve": "node dist/server/server.js"
  },
